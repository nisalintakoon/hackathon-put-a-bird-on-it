{
    "name": "Put a Bird On It",
    "version": "1.0",
    "description": "PUT A BIRD ON IT!",
    "manifest_version": 2,
    "icons": {
        "16": "./assets/green16.png",
        "32": "./assets/green32.png",
        "48": "./assets/green48.png",
        "128": "./assets/green128.png"
    },
    "background":{
        "scripts": ["./background.js"]
    },
    "options_page": "./options.html",
    "browser_action":{
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "<all_urls>"
    ]
}